	//////////////////////////////////////////////////////////////
	//
	// 诺艾尔Coop用岩元素含量高的碎石堆
	//
	//////////////////////////////////////////////////////////////
	{
		"Default": 
		{
			"$type": "ConfigAbility",
			"abilityName": "SceneObj_Environment_Rock_StonePileQuest_Hard",
			"onAdded":
			[
				{
					"$type": "AttachModifier",
					"modifierName": "BeHitBlunt",
				},
			],
			"modifiers":
			{
				"BeHitBlunt":
				{
					"elementType": "Rock",
					"elementDurability": 100,
					"modifierMixins":
					[
						{
							"$type": "ShieldBarMixin",
						},
					],
					"onBeingHit":
					[
						{
							"$type": "FireEffect",
							"effectPattern": "Eff_SceneObj_StoneWall_01_Broken",
							"born":
							{
								"$type": "ConfigBornByTarget",
							},
							"predicates":
							[
								{
									"$type":"ByNot",
									"predicates":
									[
										{
											"$type": "ByHitStrikeType",
											"strikeType": "Default",
										},
										{
											"$type": "ByHitStrikeType",
											"strikeType": "Pierce",
										},
										{
											"$type": "ByHitStrikeType",
											"strikeType": "None",
										},
									],
								},
							],
						},
					],
					"onRemoved":
					[
						{
							"$type": "FireEffect",
							"effectPattern": "Eff_SceneObj_StoneWall_01_Destroy",
							"born":
							{
								"$type": "ConfigBornByTarget",
							},
						},
						{
							"$type": "KillSelf",
						},
					],
				},
			},
		},
	},
